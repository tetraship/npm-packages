{
	"name": "@tetraship/backend",
	"version": "1.0.0",
	"description": "Tetraship backend utilities and shared modules",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./auth": {
			"types": "./dist/auth/index.d.ts",
			"default": "./dist/auth/index.js"
		},
		"./database": {
			"types": "./dist/database/index.d.ts",
			"default": "./dist/database/index.js"
		},
		"./uploads": {
			"types": "./dist/uploads/index.d.ts",
			"default": "./dist/uploads/index.js"
		},
		"./utils": {
			"types": "./dist/utils/index.d.ts",
			"default": "./dist/utils/index.js"
		}
	},
	"scripts": {
		"build": "tsc",
		"test": "vitest run",
		"clean": "rm -rf dist"
	},
	"keywords": [
		"backend",
		"tetraship",
		"utilities",
		"auth",
		"database",
		"server",
		"api",
		"s3"
	],
	"author": "Tetraship",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/tetraship/npm-packages.git",
		"directory": "packages/backend"
	},
	"publishConfig": {
		"access": "public",
		"provenance": true
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.962.0",
		"@aws-sdk/s3-request-presigner": "^3.962.0",
		"drizzle-orm": "^0.44.3",
		"libsql": "^0.5.22",
		"uuidv7": "^1.0.0",
		"zod": "^4.3.5",
		"zod-from-json-schema": "^0.5.2"
	},
	"peerDependencies": {
		"next": "^14.0.0 || ^15.0.0 || ^16.0.0",
		"next-auth": "^5.0.0",
		"react": "^18.0.0 || ^19.0.0",
		"react-dom": "^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@types/node": "^20.0.0",
		"next": "16.1.1",
		"next-auth": "5.0.0-beta.30",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"typescript": "^5.0.0"
	}
}
