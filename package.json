{
	"name": "@tetraship/npm-packages",
	"version": "1.0.0",
	"private": true,
	"description": "Monorepo for Tetraship npm packages",
	"scripts": {
		"build": "pnpm -r build",
		"test": "pnpm -r test",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format .",
		"format:fix": "biome format --write .",
		"clean": "pnpm -r clean"
	},
	"keywords": [
		"monorepo",
		"pnpm",
		"workspace"
	],
	"repository": {
		"type": "git",
